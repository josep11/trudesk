{
  "plugins": ["node"],
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "node/exports-style": ["error", "module.exports"],
    "no-console": "off"
  },
  "globals": {
    "angular": true,
    "define": true,
    "requirejs": true,
    "MG": true,
    "Snackbar": true,
    "ROLES": true,
    "io": true
  }
}
